{
  "name": "jenkins-plugin-site",
  "version": "0.0.1",
  "description": "Jenkins Plugin Site",
  "main": "index.js",
  "scripts": {
    "build": "run-s clean && run-p build:client build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.client.babel.js --colors --display-error-details",
    "build:server": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.server.babel.js --colors --display-error-details",
    "server": "run-s build server:run",
    "server:run": "node dist/server/server.js",
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --content-base public --port 5000 --inline --hot --progress --colors --history-api-fallback --watch-content-base",
    "lint": "eslint --ext .js --ext .jsx .",
    "test": "jest --coverage",
    "test:watch": "yarn test -- --watch",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": "~6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jenkins-infra/plugin-site.git"
  },
  "keywords": [
    "jenkins-ci",
    "plugins"
  ],
  "author": "Michael McCaskill <https://github.com/michaelmccaskill>",
  "contributors": [
    {
      "name": "Gus Reiber",
      "email": "https://github.com/gusreiber"
    },
    {
      "name": "Thorsten Scherler",
      "email": "https://github.com/scherler aka scherler"
    },
    {
      "name": "Michael McCaskill",
      "email": "https://github.com/michaelmccaskill"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://issues.jenkins-ci.org/secure/RapidBoard.jspa?rapidView=1&projectKey=WEBSITE&view=detail"
  },
  "homepage": "http://plugins.jenkins-ci.org/",
  "dependencies": {
    "chalk": "2.1.0",
    "chart.js": "2.6.0",
    "classnames": "2.2.5",
    "compression": "1.7.0",
    "es6-promise": "4.1.1",
    "express": "4.15.4",
    "express-handlebars": "3.0.0",
    "handlebars": "4.0.10",
    "helmet": "3.8.1",
    "hpp": "0.2.2",
    "keymirror": "0.1.1",
    "moment": "2.18.1",
    "morgan": "1.8.2",
    "node-schedule": "1.2.4",
    "react": "15.3.0",
    "react-chartjs-2": "2.5.7",
    "react-dom": "15.3.0",
    "react-header-modal": "0.4.13",
    "react-material-icons-blue": "1.0.4",
    "react-radio-group": "3.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "reselect": "3.0.1",
    "unirest": "0.5.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.1",
    "cheerio": "1.0.0-rc.2",
    "cross-env": "5.0.5",
    "css-loader": "0.28.4",
    "enzyme": "2.9.1",
    "eslint": "4.4.1",
    "eslint-plugin-react": "7.2.1",
    "extract-text-webpack-plugin": "3.0.0",
    "handlebars-loader": "1.5.0",
    "html-webpack-plugin": "2.30.1",
    "ignore-styles": "5.0.1",
    "isomorphic-fetch": "2.2.1",
    "jasmine-reporters": "2.2.1",
    "jest": "20.0.3",
    "jsdom": "11.1.0",
    "nock": "9.0.14",
    "npm-run-all": "4.0.2",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.6",
    "react-addons-test-utils": "15.3.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-mock-store": "1.2.1",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "universal-webpack": "0.3.11",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "jest": {
    "globals": {
      "__REST_API_URL__": "http://localhost:8080"
    },
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js"
  }
}

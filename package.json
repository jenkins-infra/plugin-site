    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "find-up": "^2.1.0",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jasmine-reporters": "^2.2.0",
    "jest": "^16.0.2",
    "jsdom": "^9.5.0",
    "nock": "^9.0.2",
    "npm-run-all": "^4.0.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.10.0",
    "prepend-file": "^1.3.1",
